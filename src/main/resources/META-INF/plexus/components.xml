<component-set>
	<components>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>iib-bar</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<compile>ch.sbb.maven.plugins:iib-maven-plugin:prepare-bar-build-workspace,ch.sbb.maven.plugins:iib-maven-plugin:create-bar,ch.sbb.maven.plugins:iib-maven-plugin:clean-bar-build-workspace
					</compile>
					<process-classes>ch.sbb.maven.plugins:iib-maven-plugin:validate-configurable-properties,ch.sbb.maven.plugins:iib-maven-plugin:validate-classloader-approach
					</process-classes>
					<package>ch.sbb.maven.plugins:iib-maven-plugin:package-iib-bar
					</package>
					<install>org.apache.maven.plugins:maven-install-plugin:2.4:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:2.7:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>iib-classloader</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<generate-sources>ch.sbb.maven.plugins:iib-maven-plugin:prepare-iib-classloader-packaging
					</generate-sources>
					<package>ch.sbb.maven.plugins:iib-maven-plugin:package-classloader
					</package>
					<install>org.apache.maven.plugins:maven-install-plugin:2.4:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:2.7:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>jar</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<generate-sources>ch.sbb.maven.plugins:iib-maven-plugin:prepare-jar-classpath
					</generate-sources>
					<process-resources>org.apache.maven.plugins:maven-resources-plugin:resources
					</process-resources>
					<compile>org.apache.maven.plugins:maven-compiler-plugin:compile
					</compile>
					<process-test-resources>org.apache.maven.plugins:maven-resources-plugin:testResources
					</process-test-resources>
					<test-compile>org.apache.maven.plugins:maven-compiler-plugin:testCompile
					</test-compile>
					<test>org.apache.maven.plugins:maven-surefire-plugin:test</test>
					<package>org.apache.maven.plugins:maven-jar-plugin:jar,ch.sbb.maven.plugins:iib-maven-plugin:package-src
					</package>
					<install>org.apache.maven.plugins:maven-install-plugin:2.4:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:2.7:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>iib-src</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<package>ch.sbb.maven.plugins:iib-maven-plugin:package-src
					</package>
					<install>org.apache.maven.plugins:maven-install-plugin:2.4:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:2.7:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
	</components>
</component-set>