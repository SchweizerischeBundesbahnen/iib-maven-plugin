<?xml version="1.0"?>
<!DOCTYPE component-set>
<component-set>
	<components>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>iib-bar</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<generate-resources>ch.sbb.maven.plugins:iib-maven-plugin:prepare-bar-build-workspace
					</generate-resources>
					<process-resources>ch.sbb.maven.plugins:iib-maven-plugin:validate-bar-build-workspace
					</process-resources>
					<compile>ch.sbb.maven.plugins:iib-maven-plugin:create-bar</compile>
					<process-classes>ch.sbb.maven.plugins:iib-maven-plugin:validate-configurable-properties,ch.sbb.maven.plugins:iib-maven-plugin:clean-bar-build-workspace,ch.sbb.maven.plugins:iib-maven-plugin:validate-classloader-approach
					</process-classes>
					<package>ch.sbb.maven.plugins:iib-maven-plugin:package-iib-bar
					</package>
					<install>org.apache.maven.plugins:maven-install-plugin:2.4:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:2.7:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>iib-classloader</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<generate-sources>ch.sbb.maven.plugins:iib-maven-plugin:prepare-iib-classloader-packaging
					</generate-sources>
					<package>ch.sbb.maven.plugins:iib-maven-plugin:package-classloader
					</package>
					<install>org.apache.maven.plugins:maven-install-plugin:2.4:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:2.7:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>iib-par</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<generate-sources>ch.sbb.maven.plugins:iib-maven-plugin:prepare-iib-par-packaging
					</generate-sources>
					<process-resources>org.apache.maven.plugins:maven-resources-plugin:resources
					</process-resources>
					<compile>org.apache.maven.plugins:maven-compiler-plugin:compile
					</compile>
					<process-test-resources>org.apache.maven.plugins:maven-resources-plugin:testResources
					</process-test-resources>
					<test-compile>org.apache.maven.plugins:maven-compiler-plugin:testCompile
					</test-compile>
					<test>org.apache.maven.plugins:maven-surefire-plugin:test</test>
					<package>ch.sbb.maven.plugins:iib-maven-plugin:package-par
					</package>
					<install>org.apache.maven.plugins:maven-install-plugin:2.4:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:2.7:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>jar</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<process-resources>org.apache.maven.plugins:maven-resources-plugin:resources
					</process-resources>
					<compile>org.apache.maven.plugins:maven-compiler-plugin:compile
					</compile>
					<process-test-resources>org.apache.maven.plugins:maven-resources-plugin:testResources
					</process-test-resources>
					<test-compile>org.apache.maven.plugins:maven-compiler-plugin:testCompile
					</test-compile>
					<test>org.apache.maven.plugins:maven-surefire-plugin:test</test>
					<package>org.apache.maven.plugins:maven-jar-plugin:jar,ch.sbb.maven.plugins:iib-maven-plugin:package-src
					</package>
					<install>org.apache.maven.plugins:maven-install-plugin:2.4:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:2.7:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>iib-src</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<validate>ch.sbb.maven.plugins:iib-maven-plugin:validate-project
					</validate>
					<package>ch.sbb.maven.plugins:iib-maven-plugin:package-src
					</package>
					<install>org.apache.maven.plugins:maven-install-plugin:2.4:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:2.7:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>eclipse-plugin</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<lifecycles>
					<lifecycle>
						<id>default</id>
						<phases>
							<validate>
								org.eclipse.tycho:tycho-packaging-plugin:0.22.0:build-qualifier,
								org.eclipse.tycho:tycho-packaging-plugin:0.22.0:validate-id,
								org.eclipse.tycho:tycho-packaging-plugin:0.22.0:validate-version
							</validate>
							<process-resources>
								org.apache.maven.plugins:maven-resources-plugin:2.4.3:resources
							</process-resources>
							<compile>
								org.eclipse.tycho:tycho-compiler-plugin:0.22.0:compile
							</compile>
							<process-test-resources>
								org.apache.maven.plugins:maven-resources-plugin:2.4.3:testResources
							</process-test-resources>
							<test-compile><!-- tests are always in a separate plugin -->
							</test-compile>
							<test><!-- tests are always in a separate plugin -->
							</test>
							<package>
								ch.sbb.maven.plugins:iib-maven-plugin:package-udn-jar,
								org.eclipse.tycho:target-platform-configuration:0.22.0:target-platform,
								org.eclipse.tycho:tycho-packaging-plugin:0.22.0:package-plugin,
								org.eclipse.tycho:tycho-p2-plugin:0.22.0:p2-metadata-default
							</package>
							<integration-test></integration-test>
							<install>
								org.apache.maven.plugins:maven-install-plugin:2.3.1:install,
								org.eclipse.tycho:tycho-p2-plugin:0.22.0:update-local-index
							</install>
							<deploy>
								org.apache.maven.plugins:maven-deploy-plugin:2.5:deploy
							</deploy>
						</phases>
					</lifecycle>
				</lifecycles>
			</configuration>
		</component>
	</components>
</component-set>